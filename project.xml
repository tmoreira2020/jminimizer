<?xml version="1.0" encoding="ISO-8859-1"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://maven.apache.org/xsd/maven-project-3.0.0.xsd">
  <pomVersion>3</pomVersion>
  <id>jminimizer</id>
  <name>jminimizer</name>
  <groupId>jminimizer</groupId>
  <currentVersion>alpha-2</currentVersion>
  <organization>
    <name>Federal University of Santa Catarina</name>
    <url>http://www.ufsc.br</url>
    <logo>http://www.ufsc.br/images/fundosub.gif</logo>
  </organization>
  <inceptionYear>2004</inceptionYear>
  <package>net.java.dev.jminimizer</package>
  <description>A tool to reduce the size of jar files. The target of this project is help the deployment of J2ME applications.</description>
  <shortDescription>A Java tool to reduce the size of jar files</shortDescription>
  <url>https://jminimizer.dev.java.net/</url>
  <repository>
    <connection>scm:cvs:pserver:guest@cvs.dev.java.net:/cvs:jminimizer</connection>
    <developerConnection>scm:cvs:pserver:thiagolm@cvs.dev.java.net:/cvs:jminimizer</developerConnection>
    <url>https://jminimizer.dev.java.net/source/browse/jminimizer/</url>
  </repository>
  <versions>
    <version>
      <id>alpha-1</id>
      <name>alpha-1</name>
      <tag>alpha-1</tag>
    </version>
    <version>
      <id>alpha-2</id>
      <name>alpha-2</name>
      <tag>alpha-2</tag>
    </version>
  </versions>
  <mailingLists>
    <mailingList>
      <name>${pom.name} Dev List</name>
      <subscribe>dev-subscribe@jminimizer.dev.java.net</subscribe>
      <unsubscribe>dev-unsubscribe@jminimizer.dev.java.net</unsubscribe>
      <archive>https://jminimizer.dev.java.net/servlets/SummarizeList?listName=dev</archive>
    </mailingList>
    <mailingList>
      <name>${pom.name} User List</name>
      <subscribe>users-subscribe@jminimizer.dev.java.net</subscribe>
      <unsubscribe>users-unsubscribe@jminimizer.dev.java.net</unsubscribe>
      <archive>https://jminimizer.dev.java.net/servlets/SummarizeList?listName=users</archive>
    </mailingList>
  </mailingLists>
  <developers>
    <developer>
      <name>Thiago Leão Moreira</name>
      <id>thiagolm</id>
      <email>thiagolm@dev.java.net</email>
      <organization>${pom.organization.name}</organization>
      <roles>
        <role>Project admin</role>
        <role>Java developer</role>
        <role>Bartender</role>
      </roles>
      <url>http://www.inf.ufsc.br/~timba</url>
      <timezone>-3</timezone>
    </developer>
  </developers>
  <licenses>
    <license>
      <name>GPL</name>
      <url>http://www.gnu.org/copyleft/gpl.html</url>
      <distribution>repo</distribution>
    </license>
  </licenses>
  <dependencies>
    <dependency>
      <groupId>bcel</groupId>
      <artifactId>bcel</artifactId>
      <version>5.1</version>
      <type>jar</type>
      <url>http://jakarta.apache.org/bcel/</url>
    </dependency>
    <dependency>
      <groupId>commons-logging</groupId>
      <artifactId>commons-logging</artifactId>
      <version>1.0.3</version>
      <type>jar</type>
      <url>http://jakarta.apache.org/commons/logging/</url>
    </dependency>
    <dependency>
      <groupId>regexp</groupId>
      <artifactId>regexp</artifactId>
      <version>1.3</version>
      <type>jar</type>
      <url>http://jakarta.apache.org/regexp/</url>
    </dependency>
    <dependency>
      <groupId>commons-cli</groupId>
      <artifactId>commons-cli</artifactId>
      <version>1.0</version>
      <type>jar</type>
      <url>http://jakarta.apache.org/commons/cli/</url>
    </dependency>
  </dependencies>
  <build>
    <sourceDirectory>src/java</sourceDirectory>
    <unitTestSourceDirectory>src/test/java</unitTestSourceDirectory>
    <unitTest>
      <includes>
        <include>**/*Test.java</include>
      </includes>
      <includes>
        <include>**/test*</include>
      </includes>
    </unitTest>
    <resources>
      <resource>
        <directory>resources</directory>
        <targetPath>resources</targetPath>
        <includes>
          <include>*.xsd</include>
          <include>*.dtd</include>
        </includes>
      </resource>
    </resources>
  </build>
</project>
