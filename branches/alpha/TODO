
	-eliminar os atributos que não são utilizados (métodos que foram removidos e que agora não utilizão mais os atributos).
	public class Main {

    public static void main(String[] args) throws ClassNotFoundException {
        JavaClass jc = Repository.lookupClass(args[0]);
        List ms = new LinkedList(Arrays.asList(jc.getMethods()));
        int size = ms.size();
        JavaClass sjc = jc.getSuperClass();
        while (sjc != null && ms.size() != 0) {
            an(ms, sjc, args[0]);
            sjc= sjc.getSuperClass();
        }

    }
    
    private static void an(List ms, JavaClass sjc, String className) {
        for (int i = 0; i < ms.size();) {
            //System.out.println(ms.size());
            //System.out.println(sjc.getClassName());
            org.apache.bcel.classfile.Method mcf = (org.apache.bcel.classfile.Method) ms
                    .get(i);
            Method m = new Method(className, mcf.getName(), mcf.getSignature());
            ClassGen scg = new ClassGen(sjc);
            if (scg.containsMethod(mcf.getName(), mcf.getSignature()) != null) {
                //TODO
                System.out.println("ACHOU: " + m);
                ms.remove(mcf);
            } else {
                i++;
            }
        }
    }
}
