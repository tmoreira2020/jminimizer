    <dependencies>
        <dependency>
            <groupId>bcel</groupId>
            <artifactId>bcel</artifactId>
            <version>5.1</version>
            <type>jar</type>
            <url>http://jakarta.apache.org/bcel/</url>
            <properties/>
        </dependency>
        <dependency>
